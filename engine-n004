best_next_hop_PREV_INTERNAL_[AUTOMATIC,PRIMARY=best_next_hop]: CREATE TABLE best_next_hop_PREV_INTERNAL_ (DEST varchar(128), NEXTHOP varchar(128), TOC_INTERNAL_ timestamp);

// Automatically inserted declaration(s):
// persistent policy_PREV_INTERNAL_(char[128] DEST, char[128] PATH, int PREF);
persistent
policy_PREV_INTERNAL_[AUTOMATIC,PRIMARY=policy]: CREATE TABLE policy_PREV_INTERNAL_ (DEST varchar(128), PATH varchar(128), PREF int, TOC_INTERNAL_ timestamp);

// Automatically inserted declaration(s):
// persistent best_pref_PREV_INTERNAL_(char[128] DEST, int PREF);
persistent
best_pref_PREV_INTERNAL_[AUTOMATIC,PRIMARY=best_pref]: CREATE TABLE best_pref_PREV_INTERNAL_ (DEST varchar(128), PREF int, TOC_INTERNAL_ timestamp);

// Automatically inserted declaration(s):
// persistent started_PREV_INTERNAL_(int DUMMY);
persistent
started_PREV_INTERNAL_[AUTOMATIC,PRIMARY=started]: CREATE TABLE started_PREV_INTERNAL_ (DUMMY int, TOC_INTERNAL_ timestamp);

// Automatically inserted initialization(s): 
// // Create dummy table

Initialize
DUMMIE_INTERNAL_[AUTOMATIC]: CREATE TABLE DUMMIE_INTERNAL_ (X int);


// Automatically inserted initialization(s): 
// // Load dummy table for SELECT

Initialize
DUMMIE_INTERNAL_[AUTOMATIC]: INSERT INTO DUMMIE_INTERNAL_ VALUES (1);


// 
CLAUSE

// started(X) if  start(X);
started: SELECT start.DUMMY as DUMMY, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM start;

// policy(D, P, Pref) if  add_policy(D, P, Pref);
policy: SELECT add_policy.DEST as DEST, add_policy.PATH as PATH, add_policy.PREF as PREF, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM add_policy;

// policy(D, P, Pref) if  prev policy(D, P, Pref);
policy: SELECT policy_PREV_INTERNAL_.DEST as DEST, policy_PREV_INTERNAL_.PATH as PATH, policy_PREV_INTERNAL_.PREF as PREF, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM policy_PREV_INTERNAL_;

// neighbour(N) if  add_neighbour(N);
neighbour: SELECT add_neighbour.TO_ADD as NEIGHBOUR, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM add_neighbour;

// neighbour(N) if  prev neighbour(N);
neighbour: SELECT neighbour_PREV_INTERNAL_.NEIGHBOUR as NEIGHBOUR, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM neighbour_PREV_INTERNAL_;

// block;
BLOCK

// route_updated(Dest, NextHop) if  advertise(Dest, *)@NextHop;
route_updated: SELECT advertise.DEST as DEST, advertise.SRC_INTERNAL_ as NEXTHOP, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM advertise;

// route_updated(Dest, NextHop) if  withdraw(Dest)@NextHop;
route_updated: SELECT withdraw.DEST as DEST, withdraw.SRC_INTERNAL_ as NEXTHOP, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM withdraw;

// block;
BLOCK

// candidate(Dest, NextHop, Path) if  advertise(Dest, Path)@NextHop;
candidate: SELECT advertise.DEST as DEST, advertise.SRC_INTERNAL_ as NEXTHOP, advertise.PATH as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM advertise;

// candidate(Dest, NextHop, Path) if  prev candidate(Dest, NextHop, Path), not route_updated(Dest, NextHop);
candidate: SELECT candidate_PREV_INTERNAL_.DEST as DEST, candidate_PREV_INTERNAL_.NEXTHOP as NEXTHOP, candidate_PREV_INTERNAL_.PATH as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM candidate_PREV_INTERNAL_
	WHERE (NOT EXISTS (SELECT * FROM route_updated WHERE route_updated.DEST = candidate_PREV_INTERNAL_.DEST AND route_updated.NEXTHOP = candidate_PREV_INTERNAL_.NEXTHOP));

// block;
BLOCK

// best_pref(Dest, MAX(Pref)) if  candidate(Dest, *, Path), policy(Dest, Path, Pref);
best_pref: SELECT candidate.DEST as DEST, MAX(policy.PREF) as PREF, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM candidate, policy
	WHERE candidate.DEST = policy.DEST AND candidate.PATH = policy.PATH GROUP BY candidate.DEST;

// block;
BLOCK

// best_cost(Dest, MIN(LENGTH(Path))) if  best_pref(Dest, Pref), candidate(Dest, *, Path), policy(Dest, Path, Pref);
best_cost: SELECT best_pref.DEST as DEST, MIN(LENGTH(candidate.PATH)) as COST, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_pref, candidate, policy
	WHERE best_pref.DEST = candidate.DEST AND best_pref.PREF = policy.PREF AND candidate.DEST = policy.DEST AND candidate.PATH = policy.PATH GROUP BY best_pref.DEST;

// block;
BLOCK

// best_next_hop(Dest, MIN(NextHop)) if  best_pref(Dest, BestPref), best_cost(Dest, BestCost), candidate(Dest, NextHop, Path), policy(Dest, Path, BestPref) : LENGTH(Path) == BestCost;
best_next_hop: SELECT best_pref.DEST as DEST, MIN(candidate.NEXTHOP) as NEXTHOP, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_pref, best_cost, candidate, policy
	WHERE best_pref.DEST = best_cost.DEST AND best_pref.PREF = policy.PREF AND best_cost.DEST = candidate.DEST AND candidate.DEST = policy.DEST AND candidate.PATH = policy.PATH
		AND (LENGTH(candidate.PATH) = best_cost.COST) GROUP BY best_pref.DEST;

// block;
BLOCK

// best_candidate(Dest, BestPath) if  best_next_hop(Dest, BestNextHop), candidate(Dest, BestNextHop, BestPath);
best_candidate: SELECT best_next_hop.DEST as DEST, candidate.PATH as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_next_hop, candidate
	WHERE best_next_hop.DEST = candidate.DEST AND best_next_hop.NEXTHOP = candidate.NEXTHOP;

// block;
BLOCK

// advertise(Self, Self)@N if  start(*), neighbour(N), peers(Self, "Self");
advertise: SELECT neighbour.NEIGHBOUR as DEST_INTERNAL_, ${static:self} as SRC_INTERNAL_, peers.NODE as DEST, peers.NODE as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM start, neighbour, peers
	WHERE peers.RELATIONSHIP = 'Self';

// advertise(Dest, Self + Path)@N if  neighbour(N), start(*), peers(Self, "Self"), best_candidate(Dest, Path) : LOCATE(N, Path) == 0;
advertise: SELECT neighbour.NEIGHBOUR as DEST_INTERNAL_, ${static:self} as SRC_INTERNAL_, best_candidate.DEST as DEST, peers.NODE || best_candidate.PATH as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM neighbour, start, peers, best_candidate
	WHERE peers.RELATIONSHIP = 'Self'
		AND (LOCATE(neighbour.NEIGHBOUR, best_candidate.PATH) = 0);

// advertise(Dest, Self + Path)@N if  best_candidate(Dest, Path), not prev best_candidate(Dest, Path), peers(Self, "Self"), neighbour(N) : LOCATE(N, Path) == 0;
advertise: SELECT neighbour1.NEIGHBOUR as DEST_INTERNAL_, ${static:self} as SRC_INTERNAL_, best_candidate.DEST as DEST, peers1.NODE || best_candidate.PATH as PATH, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_candidate, peers as peers1, neighbour as neighbour1
	WHERE peers1.RELATIONSHIP = 'Self'
		AND (NOT EXISTS (SELECT * FROM best_candidate_PREV_INTERNAL_ as best_candidate_PREV_INTERNAL_1 WHERE best_candidate_PREV_INTERNAL_1.DEST = best_candidate.DEST AND best_candidate_PREV_INTERNAL_1.PATH = best_candidate.PATH))
		AND (LOCATE(neighbour1.NEIGHBOUR, best_candidate.PATH) = 0);

// withdraw(Dest)@N if  prev best_candidate(Dest, *), not candidate(Dest, *, *), peers(N, "Neighbor");
withdraw: SELECT peers.NODE as DEST_INTERNAL_, ${static:self} as SRC_INTERNAL_, best_candidate_PREV_INTERNAL_.DEST as DEST, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_candidate_PREV_INTERNAL_, peers
	WHERE peers.RELATIONSHIP = 'Neighbor'
		AND (NOT EXISTS (SELECT * FROM candidate WHERE candidate.DEST = best_candidate_PREV_INTERNAL_.DEST));

// withdraw(Dest)@N if  best_candidate(Dest, Path), not prev best_candidate(Dest, Path), peers(Self, "Self"), neighbour(N) : LOCATE(N, Path) != 0;
withdraw: SELECT neighbour1.NEIGHBOUR as DEST_INTERNAL_, ${static:self} as SRC_INTERNAL_, best_candidate.DEST as DEST, CURRENT_TIMESTAMP as TOC_INTERNAL_ FROM best_candidate, peers as peers1, neighbour as neighbour1
	WHERE peers1.RELATIONSHIP = 'Self'
		AND (NOT EXISTS (SELECT * FROM best_candidate_PREV_INTERNAL_ as best_candidate_PREV_INTERNAL_1 WHERE best_candidate_PREV_INTERNAL_1.DEST = best_candidate.DEST AND best_candidate_PREV_INTERNAL_1.PATH = best_candidate.PATH))
		AND (LOCATE(neighbour1.NEIGHBOUR, best_candidate.PATH) != 0);


